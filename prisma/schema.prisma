generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int     @id @default(autoincrement())
  name        String
  mail        String
  facultyId   String  @unique
  password    String
  verifyOTP   String?
  is_verified Boolean @default(false)
}
